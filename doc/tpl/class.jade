h2=module

- if (cl.meta && cl.meta.deps)
    div.dependencies
        h3 Dependencies
        ul
            each dep in cl.meta.deps
                li=dep

- if (cl.module)
    div.description!=cl.module.description

- if (cl.constructor && cl.constructor.kind === 'class')
    div.constructor
    h3 Constructor
        a(class='srclink', href=config.repoview + cl.constructor.meta.path + '/' + cl.constructor.meta.filename + '#L' + cl.constructor.meta.lineno) View Source
        a(class='permalink', href='#', rel=cl.constructor.longName, alt='Permalink to ' + cl.constructor.longName, title='Permalink to ' + cl.constructor.longName) #
        h4
            span.name=cl.constructor.name
            span.params
                span.paren &#40;
                each param, i in cl.constructor.params
                    - if (param.optional)
                        span.opt [
                    - if (i !== 0)
                        span.sep=','
                    each type, j in param.types
                        a.paramType(href=type.link, rel=type.longName)=type.name
                        - if (j < param.types.length - 1)
                            span.or='|'
                    &nbsp;
                    span.param=param.name
                    - if (param.optional)
                        span.opt ]
                span.paren &#41;
        div.description!=cl.constructor.description

- if (cl.properties && Object.keys(cl.properties).length)
    div.properties
        h3 Properties
        each prop in cl.properties
            div.property
                a(class='srclink', href=config.repoview + prop.meta.path + '/' + prop.meta.filename + '#L' + prop.meta.lineno) View Source
                a(class='permalink', href='#', rel=prop.longName, alt='Permalink to ' + prop.longName, title='Permalink to ' + prop.longName) #
                h4
                    span.returns
                        each type, j in prop.types
                            a.returnType(href=type.link, rel=type.longName)=type.name
                            - if (j < prop.types.length - 1)
                                span.or='|'
                    &nbsp;
                    span.name=prop.name
                div.description!=prop.description

div.methods
    h3 Methods
    each method in cl.methods
        //-- console.log(JSON.stringify(method, null, 4))
        div.method
            a(class='srclink', href=config.repoview + method.meta.path + '/' + method.meta.filename + '#L' + method.meta.lineno) View Source
            a(class='permalink', href='#', rel=method.longName, alt='Permalink to ' + method.longName, title='Permalink to ' + method.longName) #
            h4(id=method.name)
                span.returns
                    each type, j in method.returns.types
                        a.returnType(href=type.link, rel=type.longName)=type.name
                        - if (j < method.returns.types.length - 1)
                            span.or='|'
                    &nbsp;
                span.name=method.name
                span.params
                    span.paren &#40;
                    each param, i in method.params
                        - if (param.optional)
                            span.opt [
                        - if (i !== 0)
                            span.sep=','
                        each type, j in param.types
                            a.paramType(href=type.link, type.longName)=type.name
                            - if (j < param.types.length - 1)
                                span.or='|'
                        &nbsp;
                        span.param=param.name
                        - if (param.optional)
                            span.opt ]
                    span.paren &#41;
            div.description!=method.description
